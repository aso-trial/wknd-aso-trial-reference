<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="AEM Sites Experience Fragment Editor"
        sling:resourceType="cq/gui/components/authoring/editors/core"
        consoleId="cq-sites-editor-experiencefragment"
        extraClientLibs="[cq.authoring.editor.sites.page,cq.authoring.editor.plugin.annotate,cq.authoring.editor.plugin.cfm,cq.authoring.editor.plugin.foundation,cq.authoring.editor.plugin.manuscript,cq.authoring.editor.plugin.dps,cq.authoring.editor.plugin.target,cq.authoring.editor.plugin.designimporter,cq.authoring.editor.plugin.msm,cq.authoring.editor.plugin.commerce,cq.authoring.editor.plugin.segment,cq.authoring.editor.plugin.campaign,cq.authoring.editor.plugin.mobile,cq.authoring.editor.plugin.screens,cq.authoring.editor.plugin.activitymap,cq.authoring.editor.sites.page.hook,cq.authoring.editor.hook,cq.authoring.editor.hook.assetfinder,cq.experiencefragments.authoring,cq.buildingblocks.authoring]"
        trackingFeature="Experience Fragment Editor">
        <content/>
        <sidepanels jcr:primaryType="nt:unstructured">
            <edit
                granite:class="js-SidePanel-content js-SidePanel-content--edit"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        margin="{Boolean}true"
                        maximized="{Boolean}true"
                        orientation="vertical"
                        size="L">
                        <items jcr:primaryType="nt:unstructured">
                            <experimentTab
                                granite:class="sidepanel-tab sidepanel-tab-experiment editor-ComponentBrowser"
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Experiments"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    headerTitle="Experiments"/>
                                <parentConfig
                                    jcr:primaryType="nt:unstructured"
                                    icon="beaker"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        granite:class="sidepanel-tab-title"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/heading"
                                        level="2"
                                        text="Experiment"/>
                                    <mfe
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/include"
                                        path="/apps/wcm/core/content/mfe"/>
                                </items>
                            </experimentTab>
                        </items>
                    </tabs>
                </items>
            </edit>
        </sidepanels>
    </jcr:content>
</jcr:root>
